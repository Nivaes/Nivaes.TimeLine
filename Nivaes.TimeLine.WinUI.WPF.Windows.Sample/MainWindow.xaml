<Window
    x:Class="Nivaes.TimeLine.WinUI.WPF.Sample.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Nivaes.TimeLine.WinUI.WPF.Sample"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:timeLine="clr-namespace:Nivaes.TimeLine.Windows;assembly=Nivaes.TimeLine.WinUI"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">

    <timeLine:TimeLineView
        x:Name="mTimeLine"
        Grid.Row="1"
        Margin="0,0,0,0"
        MarkerType="TextMarker"
        TimeLineOrientation="VerticalLeft" />
</Window>
