<Page
    x:Class="Nivaes.TimeLine.WinUI.UWP.Sample.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:Nivaes.TimeLine.WinUI.UWP.Sample"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:timeLine="clr-namespace:Nivaes.TimeLine.Windows;assembly=Nivaes.TimeLine.WinUI"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <timeLine:TimeLineView
        x:Name="mTimeLine"
        Margin="0,0,0,0"
        MarkerType="TextMarker"
        TimeLineOrientation="VerticalLeft" />
</Page>
